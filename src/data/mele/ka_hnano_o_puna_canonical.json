  "verses": [
    {
      "id": "v1",
      "type": "verse",
      "number": 1,
      "order": 1,
      "label": "Verse 1:",
      "lines": [
        {
          "id": "v1.1",
          "line_number": 1,
          "hawaiian_text": "ʻO ka   hīnano   o   Puna   kuʻu   liʻa",
          "english_text": "The hinano of Puna is my desire",
          "is_bilingual": true
        },
        {
          "id": "v1.2",
          "line_number": 2,
          "hawaiian_text": "E   lauwiliwili   ai   kaʻiʻiʻni",
          "english_text": "Twisting my emotions",
          "is_bilingual": true
        },
        {
          "id": "v1.3",
          "line_number": 3,
          "hawaiian_text": "Nipo  ninipo   hoʻi   i   ke   aloha",
          "english_text": "Yearning, longing for love",
          "is_bilingual": true
        },
        {
          "id": "v1.4",
          "line_number": 4,
          "hawaiian_text": "Ka   lehua   haʻalewa   lā     i   ke   kai",
          "english_text": "Is the lehua swaying by the sea",
          "is_bilingual": true
        }
      ]
    },
    {
      "id": "v2",
      "type": "verse",
      "number": 2,
      "order": 2,
      "label": "Verse 2:",
      "lines": [
        {
          "id": "v2.1",
          "line_number": 1,
          "hawaiian_text": "ʻO   ke   kai   nehe   mai   i   ka   uluhala",
          "english_text": "The rustling sea by the hala grove",
          "is_bilingual": true
        },
        {
          "id": "v2.2",
          "line_number": 2,
          "hawaiian_text": "E   hoʻohenoheno   nei   kuʻu   nui   kino",
          "english_text": "Gently caresses me",
          "is_bilingual": true
        },
        {
          "id": "v2.3",
          "line_number": 3,
          "hawaiian_text": "No   Puna   ka   makani   Moaniʻala",
          "english_text": "The Moani'ala wind hails from Puna",
          "is_bilingual": true
        },
        {
          "id": "v2.4",
          "line_number": 4,
          "hawaiian_text": "E   hali hali   ana   i   kuʻu   haʻehaʻe",
          "english_text": "That carries my dear affections",
          "is_bilingual": true
        }
      ]
    },
    {
      "id": "v3",
      "type": "verse",
      "number": 3,
      "order": 3,
      "label": "Verse 3:",
      "lines": [
        {
          "id": "v3.1",
          "line_number": 1,
          "hawaiian_text": "ʻEnaena   Puna   i   ke   ahi   a   pele",
          "english_text": "Puna glows in Pele’s fire",
          "is_bilingual": true
        },
        {
          "id": "v3.2",
          "line_number": 2,
          "hawaiian_text": "ʻUʻina   laka   huaʻinaʻi  Puʻu ʻŌʻō",
          "english_text": "The gushing lava crackles at Puu 'O‘o",
          "is_bilingual": true
        },
        {
          "id": "v3.3",
          "line_number": 3,
          "hawaiian_text": "Puʻō ke   ahi   lapalapa   i   luna",
          "english_text": "The blazing fire blazes up",
          "is_bilingual": true
        },
        {
          "id": "v3.4",
          "line_number": 4,
          "hawaiian_text": "Punohunohu   i   ke   ano   ahiahi",
          "english_text": "Her smoke billows in the evening",
          "is_bilingual": true
        }
      ]
    },
    {
      "id": "v4",
      "type": "verse",
      "number": 4,
      "order": 4,
      "label": "Verse 4:",
      "lines": [
        {
          "id": "v4.1",
          "line_number": 1,
          "hawaiian_text": "Haʻina     ʻia   mai   ana   ka   puana",
          "english_text": "Thus the story is told",
          "is_bilingual": true
        },
        {
          "id": "v4.2",
          "line_number": 2,
          "hawaiian_text": "No   ka   hīnano   o   Puna   paia  ʻala",
          "english_text": "For the hinano of Puna with fragrant walls",
          "is_bilingual": true
        },
        {
          "id": "v4.3",
          "line_number": 3,
          "hawaiian_text": "Hʻaina   hou   mai   ana   ka   puana",
          "english_text": "The story is told again",
          "is_bilingual": true
        },
        {
          "id": "v4.4",
          "line_number": 4,
          "hawaiian_text": "No   ka   hoa   lūlana   moe   i   ke   ala",
          "english_text": "For my peaceful friend who lies on the path",
          "is_bilingual": true
        }
      ]
    }
  ]
}